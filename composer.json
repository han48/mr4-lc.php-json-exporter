{
    "name": "mr4-lc/php-json-exporter",
    "description": "Export large datasets to a JSON file without memory exhaustion",
    "type": "library",
    "version": "0.0.1",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mr4Lc\\JsonExporter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mr4",
            "email": "zzz4everzzz@live.co.uk"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "pestphp/pest": "^1.22"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
